<% form_for @service do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :file %><br />
    <%= f.text_field :file %>
  </p>
  <p>
    <%= f.label :car_type_id %><br />
    <%= f.text_field :car_type_id %>
  </p>
  <p>
    <%= f.label :car_model %><br />
    <%= f.text_field :car_model %>
  </p>
  <p>
    <%= f.label :color %><br />
    <%= f.text_field :color %>
  </p>
  <p>
    <%= f.label :board %><br />
    <%= f.text_field :board %>
  </p>
  <p>
    <%= f.label :location_city_id %><br />
    <%= f.text_field :location_city_id %>
  </p>
  <p>
    <%= f.label :references %><br />
    <%= f.text_area :references %>
  </p>
  <p>
    <%= f.label :destination_city_id %><br />
    <%= f.text_field :destination_city_id %>
  </p>
  <p>
    <%= f.label :insured %><br />
    <%= f.check_box :insured %>
  </p>
  <p>
    <%= f.label :service_type_id %><br />
    <%= f.text_field :service_type_id %>
  </p>
  <p>
    <%= f.label :price %><br />
    <%= f.text_field :price %>
  </p>
  <p>
    <%= f.label :operator_id %><br />
    <%= f.text_field :operator_id %>
  </p>
  <p>
    <%= f.label :crane %><br />
    <%= f.text_field :crane %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
